
%html
  %head
    %title PictureManager
    = stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags

  -if current_user
    #logged_in
      %span logged in as
      = current_user.email
      = link_to 'log out', log_out_path

  %body

    = yield